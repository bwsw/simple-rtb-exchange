@startuml

node "API Host" {

    node APIImage [
        <<image>>
        <b>API</b>
    ]

    node PostgressImage [
        <<image>>
        <b>Postgress</b>
    ]

    APIImage -left- PostgressImage
}

node "UI Host" {

    node UIImage [
        <<image>>
        <b>UI</b>
    ]

    UIImage -- APIImage
}


node "RTB Host" {

    node RtbExchangeImage [
        <<image>>
        <b>RtbExchangeImage</b>
    ]
}
RtbExchangeImage -- PostgressImage

@enduml